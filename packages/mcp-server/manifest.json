{
  "dxt_version": "0.2",
  "name": "dedalus-labs-mcp",
  "version": "0.1.0-alpha.8",
  "description": "The official MCP Server for the Dedalus API",
  "author": {
    "name": "Dedalus",
    "email": "founders@dedaluslabs.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dedalus-labs/dedalus-sdk-typescript.git"
  },
  "homepage": "https://github.com/dedalus-labs/dedalus-sdk-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.dedaluslabs.ai",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "DEDALUS_API_KEY": "${user_config.DEDALUS_API_KEY}",
        "DEDALUS_X_API_KEY": "${user_config.DEDALUS_X_API_KEY}",
        "DEDALUS_ORG_ID": "${user_config.DEDALUS_ORG_ID}",
        "DEDALUS_PROVIDER": "${user_config.DEDALUS_PROVIDER}",
        "DEDALUS_PROVIDER_KEY": "${user_config.DEDALUS_PROVIDER_KEY}",
        "DEDALUS_PROVIDER_MODEL": "${user_config.DEDALUS_PROVIDER_MODEL}"
      }
    }
  },
  "user_config": {
    "DEDALUS_API_KEY": {
      "title": "api_key",
      "description": "API key for Bearer token authentication.",
      "required": false,
      "type": "string"
    },
    "DEDALUS_X_API_KEY": {
      "title": "x_api_key",
      "description": "API key for X-API-Key header authentication.",
      "required": false,
      "type": "string"
    },
    "DEDALUS_ORG_ID": {
      "title": "organization",
      "description": "Organization ID for request scoping.",
      "required": false,
      "type": "string"
    },
    "DEDALUS_PROVIDER": {
      "title": "provider",
      "description": "Provider name for BYOK mode (e.g., 'openai', 'anthropic').",
      "required": false,
      "type": "string"
    },
    "DEDALUS_PROVIDER_KEY": {
      "title": "provider_key",
      "description": "Provider API key for BYOK mode.",
      "required": false,
      "type": "string"
    },
    "DEDALUS_PROVIDER_MODEL": {
      "title": "provider_model",
      "description": "Model identifier for BYOK provider.",
      "required": false,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
